  inherited QryListagem: TZQuery
    SQL.Strings = (
      
        'select vendas.vendaId, vendas.clienteId, clientes.nome, vendas.d' +
        'ataVenda,'
      
        'vendas.totalVenda from vendas inner join clientes on clientes.cl' +
        'ienteId = vendas.clienteId')
