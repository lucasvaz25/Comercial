  inherited QryListagem: TZQuery
    SQL.Strings = (
      
        'Select p.produtoId, p.nome, p.descricao, p.valor, p.quantidade, ' +
        'p.categoriaId, c.descricao as DescricaoCategoria'
      'from produtos as p'
      'left join categorias as c on c.categoria = p.categoriaID')
